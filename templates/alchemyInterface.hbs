    <div class="alchemy-content">
      <div class="reagent-inventory">
        <div class="reagent-inventory-header">
          <select class="character-selector">
            <option value="">Select Character</option>
            {{#each characters as |character|}}
              <option value="{{character.id}}" {{#if (eq ../selectedCharacterId character.id)}}selected{{/if}}>{{character.name}}</option>
            {{/each}}
          </select>
        </div>
        <div class="reagent-list">
          {{#if selectedCharacterId}}
            {{#if reagents.length}}
              {{#each reagents}}
                <div class="reagent-item" data-uuid="{{uuid}}" draggable="true">
                  <img src="{{img}}" alt="{{name}}" class="reagent-icon" />
                  <span class="reagent-name">{{name}}</span>
                  <span class="reagent-ip-values">
                    <span class="ip-combat">C:{{ipValues.combat}}</span>
                    <span class="ip-utility">U:{{ipValues.utility}}</span>
                    <span class="ip-entropy">E:{{ipValues.entropy}}</span>
                  </span>
                </div>
              {{/each}}
            {{else}}
              <p class="no-reagents">No reagents in inventory.</p>
            {{/if}}
          {{else}}
            <p class="no-character">Please select a character.</p>
          {{/if}}
        </div>
      </div>
      <div class="cauldron">
        <h3 class="cauldron-title">Cauldron</h3>
        <div class="reagent-slots">
          {{#each slots as |slot index|}}
            <div class="reagent-slot" data-slot="{{index}}">
              <div class="reagent-name">{{slot.name}}</div>
              <div class="reagent-drop-zone" title="Drop an item here">
                {{#if slot.hasItem}}
                  <img src="{{slot.img}}" class="slotted-item-icon" alt="{{slot.name}}" />
                {{else}}
                  <span class="slot-placeholder">Drop Reagent</span>
                {{/if}}
              </div>
            </div>
          {{/each}}
        </div>
        {{#if outcomeIcons.length}}
          <div class="crafting-info" data-gold-cost="{{goldCost}}" data-base-quantity="{{baseQuantity}}">
            <span class="crafting-cost">Cost: {{goldCost}} gp</span>
            <span class="crafting-quantity" title="{{#each quantityBreakdown}}{{name}}: {{#if bonus}}+{{bonus}} consumable{{else}}No bonus{{/if}}{{#unless @last}}; {{/unless}}{{/each}}">
              Quantity: {{baseQuantity}}
            </span>
          </div>
        {{else}}
          <div class="crafting-info" data-gold-cost="0" data-base-quantity="0">
            <span class="crafting-cost">Cost: 0 gp</span>
            <span class="crafting-quantity">Quantity: 0</span>
          </div>
        {{/if}}
        <div class="ip-sums">
          <div class="ip-sum {{#if highlight.combat}}highlight{{/if}}">
            <span class="ip-label">Combat</span>
            <span class="ip-value">{{ipSums.combat}}</span>
          </div>
          <div class="ip-sum {{#if highlight.utility}}highlight{{/if}}">
            <span class="ip-label">Utility</span>
            <span class="ip-value">{{ipSums.utility}}</span>
          </div>
          <div class="ip-sum {{#if highlight.entropy}}highlight{{/if}}">
            <span class="ip-label">Entropy</span>
            <span class="ip-value">{{ipSums.entropy}}</span>
          </div>
        </div>
        <div class="outcome-icons">
          {{#each outcomeIcons}}
            <div class="outcome-container">
              <div class="outcome-text">{{outcomeName}}</div>
              <div class="outcome-icon {{#if isSelected}}selected{{/if}} {{#if hasItem}}has-item{{/if}} {{#if isUnknown}}unknown-icon{{/if}}" data-category="{{category}}" data-sum="{{sum}}" data-item-id="{{itemId}}" title="{{#if hasItem}}{{category}} {{sum}} (Shift+Click to View Item){{else}}Unknown Outcome{{/if}}">
                <img src="{{img}}" alt="{{category}} Outcome" />
              </div>
            </div>
          {{/each}}
        </div>
        <div class="crafting-controls">
          <button class="craft-btn">Craft</button>
          <button class="clear-btn">Clear</button>
        </div>
      </div>
    </div>
  </section>
</form>