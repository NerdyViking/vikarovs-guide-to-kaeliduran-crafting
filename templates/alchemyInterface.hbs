            <h3 class="rarity-label">Common Outcomes</h3>
            <div class="outcomes-grid common-grid" style="grid-template-columns: repeat(4, minmax(60px, 1fr));">
              {{#each tabs.combat.rarityGroups.common.outcomes as |outcome sum|}}
                <div class="outcome-cell" data-category="Combat" data-sum="{{sum}}">
                  {{#if outcome.itemId}}
                    <a class="outcome-icon consumable-link {{#if outcome.isUnknown}}unknown-icon{{/if}}" data-item-id="{{outcome.itemId}}" title="{{#if outcome.isUnknown}}Unknown Outcome{{else}}View Consumable{{/if}}">
                      <img src="{{outcome.itemImg}}" class="item-icon" alt="Item Icon" />
                    </a>
                  {{else}}
                    <div class="outcome-icon {{#if outcome.isUnknown}}unknown-icon{{/if}}" title="{{#if ../editMode}}Drop an item here{{else}}Unknown Outcome{{/if}}">
                      <img src="{{outcome.itemImg}}" class="item-icon" alt="Unknown Icon" />
                    </div>
                  {{/if}}
                  <span class="outcome-sum">{{sum}}</span>
                  {{#if ../editMode}}
                    {{#if outcome.itemId}}
                      <button class="clear-outcome" data-category="Combat" data-sum="{{sum}}">Clear</button>
