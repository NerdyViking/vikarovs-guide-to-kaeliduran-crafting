            {{#if cauldronSlots.[0]}}
              <img src="{{cauldronSlots.[0]}}" class="slotted-item-icon" alt="Slotted Item" />
            {{/if}}
          </div>
        </div>
        <div class="reagent-slot" data-slot="1">
          <div class="reagent-name">{{reagentNames.[1]}}</div>
