<form class="{{cssClass}} {{#if editMode}}edit-mode{{/if}}" autocomplete="off">
  <nav class="tabs interface-tabs">
    <a class="item {{#if (eq activeTab 'combat')}}active{{/if}}" data-tab="combat" title="Combat Outcomes"><i class="fas fa-swords"></i></a>
    <a class="item {{#if (eq activeTab 'utility')}}active{{/if}}" data-tab="utility" title="Utility Outcomes"><i class="fas fa-globe"></i></a>
    <a class="item {{#if (eq activeTab 'entropy')}}active{{/if}}" data-tab="entropy" title="Entropy Outcomes"><i class="fas fa-bomb"></i></a>
    {{#if groups}}
      <div class="group-selector-wrapper">
        <select class="group-selector">
          <option value="">All Campaigns</option>
          {{#each groups as |group groupId|}}
            <option value="{{groupId}}" {{#if (eq ../activeGroup groupId)}}selected{{/if}}>{{group.name}}</option>
          {{/each}}
        </select>
        <a class="edit-outcomes" title="Toggle Edit Mode"><i class="fas fa-edit"></i></a>
      </div>
    {{/if}}
  </nav>
  {{#if editMode}}
    <button class="reset-crafting-memory-btn" title="Reset Crafting Memory">
      <i class="fas fa-trash"></i> Reset Memory
    </button>
  {{/if}}
